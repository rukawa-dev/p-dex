      "version": {
        "name": "sword",
        "url": "https://pokeapi.co/api/v2/version/33/"
      }
    },
    {
      "flavor_text": "Dicen que se mantiene prácticamente inmóvil y\nen silencio porque observa el pasado y el futuro\nal mismo tiempo.",
      "language": {
        "name": "es",
        "url": "https://pokeapi.co/api/v2/language/7/"
